<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hibernate.common.ProteinData" table="protein_data">
        <id name="id" type="int" column="STOCK_ID">
            <generator class="foreign">
                <param name="property">stock</param>
            </generator>
        </id>

        <one-to-one name="stock" class="com.hibernate.common.Stock" constrained="true"/>
        <property name="total" column="total" type="int"/>
        <property name="goal" column="goal" type="int"/>

    </class>
</hibernate-mapping>