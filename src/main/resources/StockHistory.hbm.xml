<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hibernate.common.StockHistory" table="stockhistory">
        <id name="id" type="int" column="ID">
            <generator class="increment"/>
        </id>
        <many-to-one name="stock" class="com.hibernate.common.Stock" not-null="true">
            <column name="STOCK_ID"></column>
        </many-to-one>
        <property name="entryTime" column="ENTRY_TIME" type="java.util.Date"/>
        <property name="entry" column="ENTRY" type="java.lang.String"/>
    </class>
</hibernate-mapping>